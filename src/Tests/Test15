
function int array_sum(double[] c, int[] a){
    print(c[1]);
    print(a[1]);
    return a[4];
}

double[2] c = {1.0,2.5};

int[5] a;
a[1] = 16;
a[4] = 25;

int b = array_sum(c, a);
print(b);

int[6] x = {1,2,3,4,5,6};
print(x[2]);

function double multiply(int[] x, int k, int len){
    int i = len-1;
    while i>=0 {
        x[i] = x[i]*k;
        print(x[i]);
        i = i - 1;
    }
    return 1.5;
}

double z = multiply(x, 2, 6);
print(z);

x[0] = 3;
function int mul(int[] x){
    x[0] = 2;
    return x[1];
}

print(x[0]==3);
mul(x);
print(x[0]==2);
int[2] v = {5,4};
print(v[0]==5);
x[0] = 1;
mul(v);
print(x[0]==1);
print(v[0]==2);


